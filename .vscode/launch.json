    "version": "0.2.0",
    "configurations": [
        {
            "type": "java",
            "name": "Launch Appliance",
            "request": "launch",
            "mainClass": "edu.ics372.project1.appliancestore.business.entities.Appliance",
            "projectName": "Project1ApplianceStore"
        },
        {
            "type": "java",
            "name": "Launch Current File",
            "request": "launch",
            "mainClass": "${file}"
        },
        {
            "type": "java",
            "name": "Launch AutomatedTester",
            "request": "launch",
            "mainClass": "edu.ics372.project1.appliancestore.business.tests.AutomatedTester",
            "projectName": "Project1ApplianceStore",
            "vmArgs": "-enableassertions"
        },
        {
            "type": "java",
            "name": "Launch UserInterface",
            "request": "launch",
            "mainClass": "edu.ics372.project1.appliancestore.userinterface.UserInterface",
            "projectName": "Project1ApplianceStore"
        }
    ]
}